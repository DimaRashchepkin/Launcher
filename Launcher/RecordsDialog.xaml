<Window x:Class="Launcher.RecordsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
        Title="RecordsDialog" Height="300" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="11*"/>
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="okButton" Content="Ok" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Width="90" FontSize="16" Grid.Column="1" Click="OkButton_Click" Grid.Row="1"/>
        <ListBox x:Name="list" FontSize="16" Grid.RowSpan="2"/>

    </Grid>
</Window>
